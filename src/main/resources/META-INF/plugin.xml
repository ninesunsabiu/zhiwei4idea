<idea-plugin>
    <id>cn.eziolin.zhiwei4idea</id>
    <name>Zhiwei4Idea</name>
    <vendor email="ninesuns.lin@outlook.com" url="https://github.com/ninesunsabiu">Ezio Lin</vendor>

    <!--suppress PluginXmlValidity -->
    <description><![CDATA[
    <h1>zhiwei utils for idea</h1>
    ]]></description>

    <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>Git4Idea</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <completion.contributor language="TEXT" order="first, before commitCompletion" implementationClass="cn.eziolin.zhiwei4idea.VuCodeCompletionContributor"/>
        <vcsCheckinHandlerFactory implementation="cn.eziolin.zhiwei4idea.CompletionCheckinHandlerFactory"/>
        <projectService serviceInterface="cn.eziolin.zhiwei4idea.CompletionService" serviceImplementation="cn.eziolin.zhiwei4idea.CompletionServiceImpl"/>
        <applicationService serviceImplementation="cn.eziolin.zhiwei4idea.ConfigSettingsState" />
        <applicationConfigurable
                id="cn.eziolin.zhiwei4idea.PluginSettingsConfigurable"
                parentId="project.propVCSSupport.Mappings"
                instance="cn.eziolin.zhiwei4idea.PluginSettingsConfigurable"
        />
    </extensions>

</idea-plugin>